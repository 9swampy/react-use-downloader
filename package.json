{
  "name": "react-use-downloader",
  "version": "1.1.3",
  "description": "Creates a download handler function and gives progress information",
  "author": "Olavo Parno",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/olavoparno/react-use-downloader.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:coverage && istanbul-badges-readme && git add 'README.md'"
    }
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "release": "standard-version",
    "test:coverage": "jest --coverage",
    "test": "jest",
    "make-badges": "istanbul-badges-readme"
  },
  "peerDependencies": {
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@babel/runtime": "^7.13.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/react-hooks": "5.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.9",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^5.1.3",
    "istanbul-badges-readme": "^1.2.0",
    "prettier": "^2.0.5",
    "react": "^16.14.0",
    "react-dom": "^17.0.1",
    "react-scripts": "^3.4.4",
    "rollup": "^2.23.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-terser": "^7.0.0",
    "standard-version": "^8.0.2"
  }
}
